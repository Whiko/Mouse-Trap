\lstdefinestyle{styleConsole}{
  backgroundcolor=\color{Consolebluebg},
  basicstyle=\footnotesize\ttfamily\color{Consoledarkblue},       
  breakatwhitespace=false,        
  breaklines=true,          
  captionpos=b,        
  commentstyle=\color{Consoledarkblue},              
  extendedchars=true,   
  frame=lrtb,
  keepspaces=false,    
  keywordstyle=\color{Consoledarkblue},             
  numbers=none,    
  rulecolor=\color{Consoledarkblue},         
  showspaces=false,             
  showstringspaces=false,       
  stringstyle=\color{Consoledarkblue},       
  tabsize=2,
}

\lstdefinestyle{styleC}{
  backgroundcolor=\color{Cgraybg},
  basicstyle=\footnotesize\ttfamily\color{Cblue},       
  breakatwhitespace=false,        
  breaklines=true,          
  captionpos=b,                   
  commentstyle=\color{Cbluegray},       
  extendedchars=true,
  frame=lrtb,
  keepspaces=true,         
  keywordstyle=\color{Cred},       
  language=C,
  numbers=left,                  
  numbersep=5pt,                 
  numberstyle=\scriptsize\color{Cgray},
  rulecolor=\color{black},         
  showspaces=false,             
  showstringspaces=false,            
  stringstyle=\color{Cyellow},
  tabsize=2,                
  title=\lstname  
}

